<program> = whitespace* atoms whitespace*
<atoms> = (atom whitespace+)* atom
<atom> = string | number | identifier | keyword | list

<whitespace> = <#'[ \t\n]+'>

integer = #'[+-]?\d+'
real = #'[+-]?\d+\.\d*'
<number> = (integer | real)

<double-quote> = '\"'
<not-double-quote> = #'[^\"]*'
string = <double-quote> not-double-quote <double-quote>

identifier = #'[^\s"\'()\d][^\s"\'()]+'
keyword = <':'> identifier

<left-paren> = <'('>
<right-paren> = <')'>
list = whitespace* left-paren (atom | whitespace)* right-paren whitespace*
