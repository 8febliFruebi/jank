(; Beginnning with true/false. ;)
(ƒ foo (truethy boolean falsey boolean) (boolean)
  (if truethy
    (foo falsey truethy)
    falsey))

(; Containing true/false. ;)
(ƒ foos (struethy boolean sfalsey boolean) (boolean)
  (if struethy
    (foos sfalsey struethy)
    sfalsey))

(foo true false)
(foos true false)
